@model Kartverket.Register.Models.Organization

<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div class="row">
        <div class="col-lg-2 col-md-2">
            Liten logo:
        </div>
        <div class="col-lg-2 col-md-4">
            <img src="@Url.Content("~/data/organizations/" +  Model.logoFilename)" class="center-block img-small" alt="logo" />
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-2 col-md-2">
            Stor logo:
        </div>
        <div class="col-lg-2 col-md-4">
            <img src="@Url.Content("~/data/organizations/" +  Model.largeLogo)" class="center-block img-large" alt="logo" />
        </div>
    </div>
</div>
